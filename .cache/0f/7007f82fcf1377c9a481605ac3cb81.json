r(--primary-color);\n\t\ttext-decoration: none;\n\t}\n}\n\n@import './icons.scss';\n@import './main-header.scss';\n@import './hero.scss';\n@import './btn-flowium.scss';\n@import './side-by-side.scss';\n@import './genres.scss';\n@import './whoarewe.scss';\n@import './main-footer.scss';\n","src/css/icons.scss":".icon {\n\tdisplay: inline-block;\n\tbackground-size: contain;\n\tbackground-position: center;\n\tbackground-repeat: no-repeat;\n\tvertical-align: middle;\n}\n\n.icon-facebook-white {\n\twidth: 8.2px;\n\theight: 15px;\n\tbackground-image: url('../assets/img/icon-facebook-white.svg');\n}\n\n.icon-instagram-white {\n\twidth: 15px;\n\theight: 15px;\n\tbackground-image: url('../assets/img/icon-instagram-white.svg');\n}\n\n.icon-twitter-white {\n\twidth: 18.4px;\n\theight: 15px;\n\tbackground-image: url('../assets/img/icon-twitter-white.svg');\n}\n\n.icon-spotify-white {\n\twidth: 20px;\n\theight: 20px;\n\tbackground-image: url('../assets/img/icon-spotify-white.svg');\n}\n","src/css/main-header.scss":".menu {\n\tdisplay: flex;\n\tflex-direction: row;\n\tjustify-content: flex-end;\n\tlist-style-type: none;\n\tbackground-color: var(--reverse-bg-color);\n\tborder-radius: 30px;\n\tpadding: 5px 10px;\n\tmargin: 20px 0 0 0;\n\n\t&-item {\n\t\tmargin-left: 30px;\n\n\t\t&.logo {\n\t\t\tdisplay: inline-block;\n\t\t\tmargin-left: 0;\n\t\t\tmargin-right: auto;\n\t\t}\n\n\t\t&.icon-item {\n\t\t\tmargin-left: 20px;\n\t\t}\n\t}\n\n\t&-link {\n\t\tcolor: var(--reverse-text-color);\n\t\ttext-decoration: none;\n\n\t\t&:hover,\n\t\t&:focus,\n\t\t&:active {\n\t\t\tcolor: var(--primary-color);\n\t\t\ttext-decoration: none;\n\t\t}\n\n\t\t&.logo-link {\n\t\t\tdisplay: inline-block;\n\n\t\t\t&:hover,\n\t\t\t&:focus,\n\t\t\t&:active {\n\t\t\t\tcolor: var(--reverse-text-color);\n\t\t\t}\n\t\t}\n\t}\n}\n","src/css/hero.scss":".hero {\n\tpadding: 100px 0;\n\n\t&-title {\n\t\tmargin-top: 1.8em;\n\t}\n\n\t&-lead {\n\t\ttext-align: center;\n\t\tfont-size: 40px;\n\t\tposition: relative;\n\t\ttop: -25px;\n\t}\n}\n","src/css/btn-flowium.scss":".btn-flowium {\n\tdisplay: inline-block;\n\tcolor: var(--primary-color);\n\tborder: 1px solid var(--primary-color);\n\tborder-radius: 30px;\n\tpadding: 7px 10px 6px 11px;\n\tfont-size: 14px;\n\tfont-weight: 500;\n\ttext-decoration: none;\n\n\t&:hover {\n\t\tcolor: var(--main-text-color);\n\t\tbackground-color: var(--primary-color);\n\t\ttransition: 200ms;\n\t}\n}\n","src/css/side-by-side.scss":".side-by-side {\n\tbackground: center/cover url('../assets/img/background-black.svg') no-repeat;\n\tpadding: 97px 0;\n\n\t.iframe-container {\n\t\twidth: 525px;\n\t\tmargin: 0 auto;\n\t}\n\n\t&-title,\n\t&-text {\n\t\tcolor: var(--reverse-text-color);\n\t}\n}\n","src/css/genres.scss":".genres {\n\tpadding: 33px 0 28px 0;\n\n\t&-title {\n\t\ttext-align: center;\n\t}\n}\n\n.genrecard {\n\t&-img-container {\n\t\tmargin-bottom: 1.5em;\n\t}\n\t&-text {\n\t\theight: 231px;\n\t}\n}\n","src/css/whoarewe.scss":".whoarewe {\n\tbackground: center/cover url('../assets/img/background-black.svg') no-repeat\n\t\tvar(--reverse-bg-color);\n\tpadding: 100px 0 100px;\n\n\t&-wrapper {\n\t\tbackground: center/contain url('../assets/img/background-organic-gray.svg')\n\t\t\tno-repeat;\n\t\twidth: 1310px;\n\t\tmargin: 0 auto;\n\t}\n\t&-content {\n\t\tpadding: 100px 0 100px;\n\t}\n\t&-title {\n\t\tfont-size: 147px;\n\t\tmargin: 0;\n\t}\n\t&-text {\n\t\twidth: 90%;\n\t\ttext-align: center;\n\t\tfont-size: 20px;\n\t\tmargin: 0 auto 1em;\n\t}\n}\n","src/css/main-footer.scss":".main-footer {\n\tbackground-color: var(--reverse-bg-color);\n\tpadding: 30px 0;\n}\n\n.social-list {\n\tdisplay: flex;\n\tjustify-content: center;\n\tpadding: 0;\n\tmargin: 0;\n}\n.social-item {\n\tdisplay: inline-block;\n\tmargin: 0 10px;\n}\n.social-link {\n\tdisplay: inline-block;\n}\n"},"lineCount":null}},"error":null,"hash":"ae8da83ebaae18c326f0e293e1b842c6","cacheData":{"env":{}}}